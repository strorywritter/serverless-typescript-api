{
  "name": "serverless-typescript-api",
  "version": "1.0.0",
  "description": "Serverless TypeScript API with DynamoDB, S3, SNS, and Cognito",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "sam:build": "sam build",
    "sam:local": "sam local start-api",
    "sam:deploy": "sam deploy --guided",
    "sam:deploy-update":"sam build && sam deploy --no-confirm-changeset --capabilities CAPABILITY_IAM",
    "local:signup": "tsx src/signup/local-run.ts",
    "local:login": "tsx src/login/local-run.ts",
    "local:main-api": "tsx src/main-api/local-run.ts",
    "local:get-items": "tsx src/get-items/local-run.ts",
    "local:update-item": "tsx src/update-item/local-run.ts",
    "local:delete-item": "tsx src/delete-item/local-run.ts"
  },
  "keywords": [
    "serverless",
    "typescript",
    "aws",
    "lambda"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-sns": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "aws-lambda": "^1.0.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
